<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Education-PP</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <body>
    
    <nav class="navbar navbar-expand-lg bg-body-tertiary">
        <div class="container-fluid">
          <a class="navbar-brand" href="/home">Home</a>
          <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
          <div class="collapse navbar-collapse" id="navbarNav">
            <ul class="navbar-nav">
              <li class="nav-item">
                <a class="nav-link" href="#">Your Profile</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">Premium Membership</a>
              </li>
              <li class="nav-item">
                <a class="nav-link" href="#">Logout</a>
              </li>
            </ul>
          </div>
        </div>
      </nav>
      
        <% result.forEach((el) => { %>
            <div class="card" style="width: 18rem;"></div>
                <img src="..." class="card-img-top" alt="...">
                <div class="card-body">
                    <h5 class="<%= el.id %>"><%= el.name %></h5>
                    <p class="card-text">By <%= el.lecturer %></p>
                    <a href="/course/<%= el.id %>" class="btn btn-primary">See Detail</a>
                </div>
            </div>
        <% }) %>
      
  </body>
</html>